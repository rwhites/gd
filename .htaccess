RewriteOptions inherit
RewriteEngine On
RewriteBase /

RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI}

RewriteRule ^4(.+)$ dnload.php?gd=$1
RewriteRule ^1(.+)$ dlnews.php?gd=$1
RewriteRule ^5(.+)$ goto.php?dl=$1
RewriteRule ^dmca/$ dmca.php [L]
RewriteRule ^about/$ about.php [L]
RewriteRule ^contact/$ contact.php [L]
RewriteRule ^latest/$ latest.php [L]
RewriteRule ^sitemap.xml$ stmp.php [L]
RewriteRule ^file/d/(.+)$ indexxx.php?t=$1
        
Options -Indexes

ErrorDocument 400 /
ErrorDocument 401 /
ErrorDocument 402 /
ErrorDocument 403 /
ErrorDocument 404 /
